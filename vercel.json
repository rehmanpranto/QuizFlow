{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "admin.html",
      "use": "@vercel/static"
    },
    {
      "src": "login.html",
      "use": "@vercel/static"
    },
    {
      "src": "admin-login.html",
      "use": "@vercel/static"
    },
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/admin",
      "dest": "/admin.html"
    },
    {
      "src": "/login",
      "dest": "/login.html"
    },
    {
      "src": "/admin-login",
      "dest": "/admin-login.html"
    },
    {
      "src": "/api/auth(.*)",
      "dest": "/api/auth.py"
    },
    {
      "src": "/api/quiz(.*)",
      "dest": "/api/quiz.py"
    },
    {
      "src": "/api/admin(.*)",
      "dest": "/api/admin.py"
    },
    {
      "src": "/api/health",
      "dest": "/api/health.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
